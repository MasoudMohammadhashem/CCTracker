<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShapeAppearanceOverlayExtended" parent="ShapeAppearanceOverlay.Material3.FloatingActionButton">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="cornerSizeTopLeft">50%</item>
    </style>
    <style name="NapButtonStyle_fillColor" parent="ShapeAppearanceOverlay.Material3.Button">
        <item name="backgroundTint">@color/purple_500</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">10dp</item>
    </style>
    <style name="MySeekbar" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">#f4511e</item>
        <item name="android:progressTint">#388e3c</item>
        <item name="android:thumbTint">#c51162</item>
    </style>
</resources>